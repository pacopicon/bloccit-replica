<h1>All Posts</h1>
<!-- <%= @posts.inspect %> -->
<% raise unless @posts %>
<% @posts.each do |post| %>
<!-- <%= post.inspect %> -->
<div class="media">
  <div class="media-body">
    <h4 class="media-heading">
      <% raise "Found post without id: #{post.inspect}" unless post.id %>
      <p><%= link_to post.title, post %></p>
      <!-- <p><%= link_to post.title, post_path(post) %></p> -->
      <!-- <p><%= link_to post.title, post_path(post.id) %></p> -->
    </h4>
  </div>
</div>
<% end %>
